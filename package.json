{
  "name": "coo-dashboard",
  "version": "1.0.0",
  "description": "COO Dashboard для Headcorn/Megamind",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && ([ -n \"$DATABASE_URL\" ] && prisma db push --accept-data-loss || true) && next build",
    "start": "next start -p ${PORT:-3000}",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^3.0.3",
    "lucide-react": "^0.468.0",
    "next": "^15.1.4",
    "next-auth": "^4.24.13",
    "otpauth": "^9.4.1",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@railway/cli": "^4.25.3",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
